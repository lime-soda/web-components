{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.js"
      ]
    },
    "build:types": {
      "dependsOn": [
        "^build:types"
      ],
      "outputs": [
        "dist/**/*.d.ts"
      ]
    },
    "build:manifest": {
      "outputs": [
        "custom-elements.json"
      ]
    }
  }
}
