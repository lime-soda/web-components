{
  "transition": {
    "$type": "transition",
    "emphasis": {
      "$value": {
        "duration": "{transition.duration.medium}",
        "delay": "{transition.duration.instant}",
        "timingFunction": "{transition.easingFunction.accelerate}"
      }
    },
    "fade": {
      "$value": {
        "duration": "{transition.duration.long}",
        "delay": "{transition.duration.instant}",
        "timingFunction": "{transition.easingFunction.decelerate}"
      }
    },
    "easingFunction": {
      "$type": "cubicBezier",
      "accelerate": {
        "$value": [0.5, 0, 1, 1]
      },
      "decelerate": {
        "$value": [0, 0, 0.5, 1]
      }
    },
    "duration": {
      "$type": "duration",
      "instant": {
        "$value": "0ms"
      },
      "short": {
        "$value": "100ms"
      },
      "medium": {
        "$value": "300ms"
      },
      "long": {
        "$value": "600ms"
      }
    }
  }
}
